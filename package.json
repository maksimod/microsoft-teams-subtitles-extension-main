{
  "name": "teams-subtitle-translator",
  "version": "1.0.0",
  "description": "Chrome extension that translates Microsoft Teams subtitles",
  "main": "src/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "dev": "cross-env NODE_ENV=development webpack --watch --config webpack.config.js",
    "build:win": "set NODE_ENV=production && webpack --config webpack.config.js",
    "dev:win": "set NODE_ENV=development && webpack --watch --config webpack.config.js"
  },
  "keywords": [
    "teams",
    "translation",
    "subtitles",
    "extension"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/runtime": "^7.22.5",
    "babel-loader": "^9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.31.0",
    "cross-env": "^7.0.3",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.5"
  }
}
